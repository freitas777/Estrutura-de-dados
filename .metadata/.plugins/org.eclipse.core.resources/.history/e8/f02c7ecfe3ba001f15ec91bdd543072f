package higeia;

public class No {
	    private String nome;
	    private No proximo;
	    private No anterior;

	    public No(String nome) {
	        this.nome = nome;
	    }

	    public String getNome() {
	        return nome;
	    }

	    public No getProximo() {
	        return proximo;
	    }

	    public void setProximo(No proximo) {
	        this.proximo = proximo;
	    }

	    public No getAnterior() {
	        return anterior;
	    }

	    public void setAnterior(No anterior) {
	        this.anterior = anterior;
}
