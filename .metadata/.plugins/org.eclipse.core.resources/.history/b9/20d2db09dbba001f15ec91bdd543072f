package higeia;

public class Exame {
	
    public carro(String paciente, String tipo, int tamanho) {
    	this.getPaciente(paciente);
    	this.getTipo(tipo);
    	this.setTamanho;
    }
        public void adicionarExame(String paciente, String tipo, String data) throws Exception {
        	if (pilhaVazia()) {
        		throw new Exception("SEM_EXAME");
        }
        	toString();
    	        
        }
        public boolean pilhaVazia() {
            if (this.tamanho == -1) {
                return true;
            }
            return false;
    }
        
        public Exame visualizarExameMaisRecente() throws Exception {
            if (exame.isEmpty()) {
                throw new Exception("SEM_EXAMES");
            }
            return exame.peek();
        }

        public Exame removerExameMaisRecente() throws Exception {
            if (exame.isEmpty()) {
                throw new Exception("SEM_EXAMES");
            }
            return exames.pop();
        }
    }
		public String getPaciente() {
		    return paciente;
		}
		
		public String getTipo() {
		    return tipo;
		}
		
		public String getData() {
		    return data;
}
}
